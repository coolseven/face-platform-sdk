{
  "name": "coolseven/face-platform-sdk",
  "description": "Face Platform Api Client For Laravel",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "coolseven",
      "email": "coolseven2013@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^6.0",
    "illuminate/cache": "^6.0",
    "illuminate/events": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Coolseven\\FacePlatformSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coolseven\\FacePlatformSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coolseven\\FacePlatformSdk\\FacePlatformServiceProvider"
      ]
    }
  }
}
